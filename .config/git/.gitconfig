[user]
	name = Max Petretta
	email = max@maxpetretta.com
[core]
	editor = nvim
[init]
  defaultBranch = master
[fetch]
  prune = true
[pull]
  rebase = true
  merge = false
[alias]
	graph = log --all --decorate --oneline --graph
	long = log --all --decorate --graph --abbrev-commit --date=relative
[credential]
  helper = osxkeychain
[credential "https://github.com"]
	helper =
	helper = !/opt/homebrew/bin/gh auth git-credential
[credential "https://gist.github.com"]
	helper =
	helper = !/opt/homebrew/bin/gh auth git-credential
